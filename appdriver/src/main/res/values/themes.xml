<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GuanzonCircle" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <!--        <item name="colorPrimary">@color/guanzon_orange</item>fefefe-->
        <item name="colorPrimaryVariant">@color/colorPrimaryVariantLight</item>
        <item name="colorOnPrimary">@color/material_white</item>
        <!--        <item name="colorAccent">@color/light_grey</item>-->

<!--        <item name="colorAccent">@color/guanzon_orange</item>-->
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorSecondary</item>
        <item name="colorSecondaryVariant">@color/colorSecondary</item>
        <item name="colorOnSecondary">@color/material_black</item>
        <item name="android:windowAnimationStyle">@style/WindowAnimationTransition</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/status_bar_default_color</item>
        <!-- Customize your theme here. -->
        <item name="android:colorBackground">@color/background_light</item>
    </style>

    <style name="Theme.GuanzonCircle.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.GuanzonCircle.PopupOverlay" parent="ThemeOverlay.AppCompat.DayNight"/>

    <style name="Theme.GuanzonCircle.AppBarOverlay" parent="ThemeOverlay.AppCompat.DayNight.ActionBar">

        <!--        <item name="android:actionMenuTextColor">@color/toolbarTextColorLight</item>-->

    </style>

    <style name="TransparentToolbar" parent="Theme.GuanzonCircle.NoActionBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="windowActionBarOverlay">true</item>
        <item name="colorPrimary">@android:color/transparent</item>
    </style>

    <style name="ToolbarTitle">
        <item name="android:typeface">serif</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/material_black</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="Theme.GuanzonCircle.TextInputContainer" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:layout_margin">3dp</item>
        <item name="android:padding">5dp</item>
        <item name="boxCornerRadiusTopStart">8dp</item>
        <item name="boxCornerRadiusTopEnd">8dp</item>
        <item name="boxCornerRadiusBottomStart">8dp</item>
        <item name="boxCornerRadiusBottomEnd">8dp</item>
    </style>

    <style name="Theme.GuanzonCircle.TextInput">
        <item name="android:fontFamily">@font/roboto_regular</item>
    </style>

    <style name="Theme.GuanzonCircle.TextInputAutoComplete">
        <item name="android:textColor">@color/material_black</item>
        <item name="android:textSize">14sp</item>
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:padding">15dp</item>
        <item name="android:inputType">textAutoComplete</item>
    </style>

    <style name="Theme.GuanzonCircle.HeaderLabel">
        <item name="android:textColor">@color/material_black</item>
        <item name="android:textSize">18sp</item>
        <item name="fontFamily">@font/roboto_medium</item>
        <item name="android:padding">15dp</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <style name="WindowAnimationTransition">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>

    </style>

    <style name="PopupAnimation">
        <item name="android:windowEnterAnimation">@anim/anim_pop_in</item>
        <item name="android:windowExitAnimation">@anim/anim_pop_out</item>
    </style>

    <style name="HeaderLabelBlack">
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:textColor">@color/material_white</item>
        <item name="fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="HeaderLabelBlack1">
        <item name="android:textColor">@color/material_black</item>
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="CustomDropDownTilStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">2dp</item>
        <item name="boxCornerRadiusBottomEnd">10dp</item>
        <item name="boxCornerRadiusBottomStart">10dp</item>
        <item name="android:textColor">@color/material_black</item>
        <item name="boxCornerRadiusTopStart">10dp</item>
        <item name="boxCornerRadiusTopEnd">10dp</item>
    </style>

    <style name="TextInputStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:layout_margin">2dp</item>
        <item name="boxCornerRadiusBottomEnd">10dp</item>
        <item name="boxCornerRadiusBottomStart">10dp</item>
        <item name="boxCornerRadiusTopStart">10dp</item>
        <item name="boxCornerRadiusTopEnd">10dp</item>
    </style>

    <style name="TextInputStyle.Edittext">
        <item name="iconStartPadding">10dp</item>
        <item name="android:drawablePadding">10dp</item>
    </style>

    <style name="AutoCompleteStyleTheme">
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="paddingStart">10dp</item>
        <item name="maxLines">1</item>
    </style>

